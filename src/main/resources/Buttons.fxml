<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.app.wine2.ButtonsController"
      alignment="CENTER" spacing="10" style="-fx-alignment: center;">
    <ComboBox fx:id="colorComboBox" prefHeight="44.0" prefWidth="182.0">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Red"/>
                <String fx:value="White"/>
            </FXCollections>
        </items>
    </ComboBox>
    <Button onAction="#handleFetchpH" prefHeight="52.0" prefWidth="182.0" text="Fetch pH"/>
    <Button onAction="#handleCounterRedWhite" prefHeight="44.0" prefWidth="182.0" text="Counter Red/White" />
    <Button onAction="#handleSelectMaxMin" prefHeight="44.0" prefWidth="182.0" text="Select Max/Min" />
    <Button onAction="#handlePHSmallerThan6" prefHeight="44.0" prefWidth="182.0" text="pH &lt; 6" />
</VBox>
